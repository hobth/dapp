<script>

function list() {
    return {
        collmenu:false,
        collections: supported_collection,
        collection:'0x1e988ba4692e52bc50b375bcc8585b95c48aad77',
        arr:{},
        async init() {
            tokens = await getNFTCollection(this.collection);
            tokens.forEach(async token => {
                obj = await getNFTToken(this.collection, token.token_id)
                console.log('obj',obj)
                this.arr[token.token_id] = obj.nft_data[0];
                console.log(JSON.parse(JSON.stringify(this.arr[token.token_id])))
            })
        }
    }
}



</script> 